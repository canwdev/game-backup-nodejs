{
  "name": "game-backup-nodejs",
  "version": "1.1.1",
  "repository": "https://github.com/canwdev/game-backup-nodejs.git",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "bun --watch run src/main.ts",
    "dev:editor": "bun run docs/index.html",
    "build": "bun build --target=node --format=cjs --minify src/main.ts --outfile ./dist/game-backup-cli.cjs",
    "build:exe": "bun build --compile --minify --sourcemap --bytecode src/main.ts --outfile ./dist/game-backup-cli",
    "lint": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "enquirer": "^2.4.1",
    "which": "^5.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@types/node": "^24.7.2",
    "@types/which": "^3.0.4",
    "eslint": "^9.37.0"
  }
}
